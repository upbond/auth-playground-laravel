# Ignore the vendor directory
/vendor

# Ignore any logs
/storage/logs/*.log

# Ignore any user-specific files
/storage/*.key
/storage/*.pem

# Ignore any environment-specific files
.env

# Ignore any session, cache, and views
/storage/*.php
/bootstrap/cache/*.php

# Ignore any files generated by editors
.idea
*.iml
.vscode

# Ignore any user uploads or generated files
/public/storage
/public/hot
/public/*.map

# Ignore any dependencies
/node_modules

# framework
/storage/framework/cache/data/*
!/storage/framework/cache/.gitkeep
/storage/framework/sessions/*
!/storage/framework/sessions/.gitkeep
/storage/framework/views/*.php
!/storage/framework/views/.gitkeep

# public
/storage/app/public/*
!/storage/app/public/.gitkeep